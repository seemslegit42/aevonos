// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  // NOTE: If you are using the free tier of a service like Neon,
  // you may need to add a `relationMode = "prisma"` option here.
}

generator client {
  provider = "prisma-client-js"
}

model Contact {
  id        String   @id @default(cuid())
  firstName String?
  lastName  String?
  email     String?  @unique
  phone     String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
